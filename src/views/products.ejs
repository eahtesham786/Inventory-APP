<table class="table table-dark">
  <thead>
    <tr>
      <th scope="col">ID</th>
      <th scope="col">Name</th>
      <th scope="col">Description</th>
      <th scope="col">Price</th>
      <th scope="col">Image</th>
    </tr>
  </thead>
  <tbody>
    <% products.forEach(p=>{ %>
    <tr>
      <th><%= p.id %></th>
      <td><%= p.name %></td>
      <td><%= p.desc %></td>
      <td scope="col"><%= p.price %></td>
      <td>
        <img src="<%= p.imageUrl %>" />
      </td>
    </tr>
    <% }) %>
  </tbody>
</table>
